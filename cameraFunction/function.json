{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "photo-in/{DateTime}.jpg",
      "connection": "AzureWebJobsStorage",
      "direction": "out",
      "datatype": "binary"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}