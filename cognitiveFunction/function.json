{
  "disabled": false,
  "bindings": [
    {
      "name": "triggerBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "photo-in/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "photo-out/{DateTime}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out",
      "dataType": "string"
    }
  ]
}